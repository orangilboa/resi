{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "legacyDecorators": "true"
    }
  },
  "rules": {
    "semi": ["error", "always"],
    "no-console": "warn",
    "no-unused-vars": 1,
    "guard-for-in":"off"
  },
  "settings": {}
}
